{% extends 'base.html.twig' %}
{% block title %}Tracks | {{ parent() }}{% endblock %}
{% block body %}
<h1> {{ title }} </h1>

<div>
    Tracks:
    <ul>
        {% for track in tracks %}
        <li>
            {{ track.song }} - {{ track.artist }}
        </li>
        {% endfor %}
    </ul>
</div>
{% endblock %}